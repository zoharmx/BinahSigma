{
  "rewrites": [
    { "source": "/", "destination": "/frontend/index.html" },
    { "source": "/app", "destination": "/frontend/app.html" },
    { "source": "/app.html", "destination": "/frontend/app.html" },
    { "source": "/v1", "destination": "/frontend/index_v1.html" },
    { "source": "/index_v1.html", "destination": "/frontend/index_v1.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
